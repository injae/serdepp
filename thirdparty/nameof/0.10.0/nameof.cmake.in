# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(nameof-0.10.0-install)

set(CPPM_VERSION 0.0.13)
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(nameof 0.10.0  URL https://github.com/Neargye/nameof/archive/refs/tags/v0.10.0.tar.gz  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} NAMEOF_OPT_BUILD_EXAMPLES=OFF NAMEOF_OPT_BUILD_TESTS=OFF)

